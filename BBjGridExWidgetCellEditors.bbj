class public BBjGridExWidgetCellEditorBasicDate extends BBjGridExWidgetCellEditorBasicDateTime
    rem /**
    rem  * Construct a new BBjGridExWidgetCellEditorBasicDate
    rem  */
    method public BBjGridExWidgetCellEditorBasicDate()
        #setEnableTime(0)
        #setMask("%Y/%Mz/%Dz")
    methodend
    rem /**
    rem  * Construct new BBjGridExWidgetCellEditorBasicDate
    rem  *
    rem  * @param mask! BBj date mask
    rem  */
    method public BBjGridExWidgetCellEditorBasicDate(BBjString mask!)
        #super!(mask!)
        #setEnableTime(0)
    methodend

classend
rem /**
rem  * A date cell editor
rem  *
rem  * @author Hyyan Abo Fakher
rem  */
class public BBjGridExWidgetCellEditorBasicTime extends BBjGridExWidgetCellEditorBasicDateTime
    rem /**
    rem  * Construct a new BBjGridExWidgetCellEditorBasicTime
    rem  */
    method public BBjGridExWidgetCellEditorBasicTime()
        #setEnableTime(1)
        #setEnableCalendar(0)
        #setMask("%Hz:%mz:%sz")
    methodend
    rem /**
    rem  * Construct new BBjGridExWidgetCellEditorBasicTime
    rem  *
    rem  * @param mask! BBj date mask
    rem  */
    method public BBjGridExWidgetCellEditorBasicTime(BBjString mask!)
        #super!(mask!)
        #setEnableTime(1)
        #setEnableCalendar(0)
    methodend

classend