rem /**
rem  * Simple editors that use the standard HTML select tag.
rem  *
rem  * The standard HTML select to behave oddly in the grid. This is because the browser doesn't have a great API for
rem  * opening and closing the select's popup.
rem  *
rem  * We advise that you don't use it unless you have to - that is we advise against BBjGridExWidgetCellEditorAGSelect
rem  * as they give poor user experience, especially if using keyboard navigation.
rem  *
rem  * If using Enhanced grid , you should use the provided BBjGridExWidgetCellEditorAGRichSelect.
rem  *
rem  * @author Hyyan Abo Fakher
rem  */
class public BBjGridExWidgetCellEditorAGSelect extends BBjGridExWidgetCellEditorAbstract
    rem /**
    rem  * An array of values to allow the user to select from
    rem  */
    field public JsonArray Values! = new JsonArray()
    rem /**
    rem  * {@inheritDoc}
    rem  */
    method public static BBjString getCellEditorName()
        methodret "agSelectCellEditor"
    methodend
    rem /**
    rem  * {@inheritDoc}
    rem  */
    method public JsonObject getAsJsonObject()
        declare JsonObject json!

        json! = new JsonObject()
        json!.add("values", iff(#getValues().size() > 0 , #getValues() , empty!), err=*next)
        methodret json!
    methodend

classend
rem /**
rem  * Simple editors that use the standard HTML select tag.
rem  *
rem  * It is same as BBjGridExWidgetCellEditorAGSelect but as popup.
rem  *
rem  * The standard HTML select to behave oddly in the grid. This is because the browser doesn't have a great API for
rem  * opening and closing the select's popup.
rem  *
rem  * We advise that you don't use it unless you have to - that is we advise against BBjGridExWidgetCellEditorAGPopupSelect
rem  * as they give poor user experience, especially if using keyboard navigation.
rem  *
rem  * If using Enhanced grid, you should use the provided BBjGridExWidgetCellEditorAGRichSelect.
rem  *
rem  * <br><b><small>#OnlyGUI</small></b>
rem  * <br><b><small>#Enterprise</small></b>
rem  *
rem  * @author Hyyan Abo Fakher
rem  */
class public BBjGridExWidgetCellEditorAGPopupSelect extends BBjGridExWidgetCellEditorAGSelect
    rem /**
    rem  * Check if it is bui env
    rem  *
    rem  * @return BBjNumber
    rem  */
    method protected static BBjNumber isBUI()
        methodret info(3,6) = "5"
    methodend
    rem /**
    rem  * {@inheritDoc}
    rem  */
    method public static BBjString getCellEditorName()
        name! = "agPopupSelectCellEditor"

        if(#isBUI())
            name! = BBjGridExWidgetCellEditorAGSelect.getCellEditorName()
        FI

        methodret name!
    methodend

classend
rem /**
rem  * Available in Enhacned Grid only. An alternative to using the browser's select popup for dropdowns inside the grid.
rem  *
rem  * <br><b><small>#OnlyGUI</small></b>
rem  * <br><b><small>#Enterprise</small></b>
rem  *
rem  * @author Hyyan Abo Fakher
rem  */
class public BBjGridExWidgetCellEditorAGRichSelect extends BBjGridExWidgetCellEditorAGSelect
    rem /**
    rem  * Provide a cell renderer for each value
    rem  */
    field public BBjGridExWidgetRendererInterface CellRenderer! = null()
    rem /**
    rem  * Check if it is bui env
    rem  *
    rem  * @return BBjNumber
    rem  */
    method protected static BBjNumber isBUI()
        methodret info(3,6) = "5"
    methodend
    rem /**
    rem  * {@inheritDoc}
    rem  */
    method public static BBjString getCellEditorName()
        name! = "agRichSelectCellEditor"

        if(#isBUI())
            name! = BBjGridExWidgetCellEditorAGSelect.getCellEditorName()
        FI

        methodret name!
    methodend
    rem /**
    rem  * {@inheritDoc}
    rem  */
    method public JsonObject getAsJsonObject()
        declare JsonObject json!

        json! = #super!.getAsJsonObject()
        renderer! = #getCellRenderer()

        if(renderer! <> null()) then
            json!.addProperty("cellRenderer" , renderer!.getCellRendererName(),err=*next)
            json!.        add("cellRendererParams" , renderer!.getAsJsonObject(),err=*next)
        FI

        methodret json!
    methodend

classend
