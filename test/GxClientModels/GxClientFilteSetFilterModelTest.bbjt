    rem @Test
    method public void testGetMiniFilterExecuteCode()
        #Instance!.setColumn("foo")
        widget! = #Instance!.getWidget()
        
        filter! = #Instance!.getMiniFilter()
        Assert.Equals(filter! , "")
        
        widget!.setIsReady(BBjAPI.TRUE)
        #Instance!.getMiniFilter()
